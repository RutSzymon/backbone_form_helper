<select <%- @unfold_options %> id="<%= @field_id %>" name="<%= @field_name %>">
  <% if @placeholder?: %><option disabled selected style="display: none;"><%= @placeholder %><% end %>
  <% if @unfold_options.indexOf(["include_blank=true"]) != -1: %> <option></option> <% end %>
  <% for option in @values: %>
    <option data="<%= @value %>" <%= "selected" if @value is option[0] %> value="<%= option[0] %>"><%= option[1] %></option>
  <% end %>
</select>
